{
  "db": {
    "connectionString": "postgres://user:secret@database",
    "provider": "postgres"
  },

  "ari": {
    "url": "http://localhost:8088",
    "username": "user",
    "passworde": "password",
    "applicationName": "test"
  },

  "prompts": {
    "mailboxWriter": {
      "greeting": [{
        "sound": "sound:vm-theperson",
        "skipable": false,
        "postSilence": 1
      }, {
        "sound": "characters:{extension}",
        "skipable": false,
        "postSilence": 1
      }, {
        "sound": "sound:{status}",
        "skipable": false,
        "postSilence": 0
      }],

      "intro": [{
        "sound": "sound:vm-intro",
        "skipable": true,
        "postSilence": 1
      }, {
        "sound": "sound:beep",
        "skipable": true,
        "postSilence": 0
      }],

      "goodbye": [{
        "sound": "sound:vm-goodbye",
        "skipable": false,
        "postSilence": 0
      }]
    }
  },

  "inputs": {
    "mailboxWriter": {
      "ready": {
        "#": "stop"
      }
    }
  },

  "mailbox": {
    "msg_format": "wav",
    "min_sec": "10"
  }
}

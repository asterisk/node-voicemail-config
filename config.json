{
  "db": {
    "connectionString": "postgres://user:secret@database",
    "provider": "postgres"
  },

  "ari": {
    "url": "http://localhost:8088",
    "username": "user",
    "password": "password",
    "applicationName": "test"
  },

  "prompts": {
    "mailboxWriter": {
      "greeting": [{
        "sound": "sound:vm-theperson",
        "skipable": true,
        "postSilence": 0
      }, {
        "sound": "characters:{extension}",
        "skipable": true,
        "postSilence": 0
      }, {
        "sound": "sound:{status}",
        "skipable": true,
        "postSilence": 1
      }],

      "intro": [{
        "sound": "sound:vm-intro",
        "skipable": true,
        "postSilence": 1
      }, {
        "sound": "sound:beep",
        "skipable": true,
        "postSilence": 0
      }],

      "goodbye": [{
        "sound": "sound:vm-goodbye",
        "skipable": false,
        "postSilence": 0
      }]
    },

    "mailboxReader": {
      "introMessages": [{
        "sound": "sound:vm-youhave",
        "skipable": true,
        "postSilence": 0
      }, {
        "sound": "number:{messageCount}",
        "skipable": true,
        "postSilence": 0
      }, {
        "sound": "{folder}",
        "skipable": true,
        "postSilence": 0
      }, {
        "sound": "sound:vm-message{plural}",
        "skipable": true,
        "postSilence": 0
      }],

      "introNoMessages": [{
        "sound": "sound:vm-no",
        "skipable": true,
        "postSilence": 0
      }, {
        "sound": "{folder}",
        "skipable": true,
        "postSilence": 0
      }, {
        "sound": "sound:vm-messages",
        "skipable": true,
        "postSilence": 0
      }],

      "messageInfoPre": [{
        "sound": "{order}",
        "skipable": true,
        "postSilence": 0
      }, {
        "sound": "sound:vm-message",
        "skipable": true,
        "postSilence": 0
      }],

      "messageInfoPost": [{
        "sound": "sound:vm-message",
        "skipable": true,
        "postSilence": 0
      }, {
        "sound": "{order}",
        "skipable": true,
        "postSilence": 0
      }],

      "noMore": [{
        "sound": "sound:vm-nomore",
        "skipable": true,
        "postSilence": 0
      }],

      "message": [{
        "sound": "recording:{recording}",
        "skipable": true,
        "postSilence": 0
      }],

      "menuFirst": [{
        "sound": "sound:vm-press",
        "skipable": true,
        "postSilence": 0
      },{
        "sound": "number:1",
        "skipable": true,
        "postSilence": 0
      },{
        "sound": "sound:vm-for",
        "skipable": true,
        "postSilence": 0
      },{
        "sound": "{folder}",
        "skipable": true,
        "postSilence": 0
      }, {
        "sound": "sound:vm-messages",
        "skipable": true,
        "postSilence": 0
      }],

      "menuRepeat": [{
        "sound": "sound:vm-repeat",
        "skipable": true,
        "postSilence": 0
      }],

      "menuNext": [{
        "sound": "sound:vm-next",
        "skipable": true,
        "postSilence": 0
      }],

      "menuPrev": [{
        "sound": "sound:vm-prev",
        "skipable": true,
        "postSilence": 0
      }],

      "menuDelete": [{
        "sound": "sound:vm-delete",
        "skipable": true,
        "postSilence": 0
      }]
    }
  },

  "inputs": {
    "mailboxWriter": {
      "ready": {
        "#": "stop"
      }
    },

    "mailboxReader": {
      "ready": {
        "1": "first",
        "4": "prev",
        "5": "replay",
        "6": "next",
        "7": "delete"
      }
    }
  },

  "mailbox": {
    "msg_format": "wav",
    "min_sec": "10"
  }
}
